{
  "name": "electron-pack",
  "version": "1.1.2",
  "description": "jixin",
  "main": "index.js",
  "scripts": {
    "start": "electron ./",
    "pack": "electron-packager ./ jixin --platform=win32 --arch=x64 --app-version=1.0.0 --out=./jixin --electron-version=9.0.2 --icon=logo.ico --asar",
    "test": "npm run build && mocha -t 5000",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "Electron",
    "pack"
  ],
  "author": "orange",
  "license": "CC0-1.0",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "electron": "^9.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.3"
  },
  "dependencies": {
    "asar": "^3.0.3",
    "electron-packager": "^14.2.1",
    "nyc": "^15.1.0"
  }
}
